# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/agent_factory"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-generate-with-openssl-rand-base64-32"

# GitHub OAuth App
# Create at: https://github.com/settings/developers
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"

# GitHub Webhook Secret
# Set when creating webhook at: https://github.com/settings/hooks
GITHUB_WEBHOOK_SECRET="your-webhook-secret"

# App Configuration
NODE_ENV="development"
PORT=3000
# Preview Panel
# URL of the running app to preview (usually same as NEXTAUTH_URL in dev)
NEXT_PUBLIC_PREVIEW_URL="http://localhost:3000"

# Dev Auth Bypass (set to "true" to enable demo mode bypass in development)
NEXT_PUBLIC_DEV_AUTH_BYPASS="false"

# AFC-C2-MOLTBOT-INGEST-1: Moltbot Ingest Token (internal only)
# Generate with: openssl rand -base64 32
# This token authenticates Moltbot -> AFC C2 ingest requests
MOLTBOT_INGEST_TOKEN=""
